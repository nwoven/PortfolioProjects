SELECT
	draft_year,
	count(draft_year) as Number_of_Draftees
FROM
	common_player_info
GROUP BY
	draft_year
ORDER BY
	Number_of_Draftees DESC</sql><sql name="SQL 3">-- Number of undrafted players per year
SELECT
	draft_year,
	from_year,
	count(from_year) as count
FROM
	common_player_info
WHERE
	draft_year = &quot;Undrafted&quot;
GROUP BY
	from_year
ORDER BY count DESC</sql><sql name="SQL 4">-- All first round draft picks
SELECT
	display_first_last,
	position,
	draft_year,
	draft_round
FROM
	common_player_info
WHERE
	draft_round is 1
ORDER BY
	draft_year</sql><sql name="SQL 5">-- Inner join to combine player information
SELECT
	d.season,
	d.first_name,
	d.last_name,
	c.birthdate,
	d.height_wo_shoes_ft_in,
	d.weight,
	c.position,
	d.standing_reach_ft_in,
	d.standing_vertical_leap,
	d.max_vertical_leap,
	d.lane_agility_time,
	c.draft_round
FROM
	common_player_info as c
	INNER JOIN draft_combine_stats as d
ON
	d.first_name = c.first_name
WHERE
	c.draft_year = d.season
ORDER BY
	d.last_name</sql><sql name="SQL 6">-- Number of total wins and losses of the Cleveland Cavaliers
SELECT
count(CASE 
	WHEN wl_home IS &quot;W&quot; AND team_abbreviation_home IS &quot;CLE&quot; THEN &quot;W&quot; END) as &quot;W_Home&quot;,
count(CASE 
	WHEN wl_home IS &quot;L&quot; AND team_abbreviation_home IS &quot;CLE&quot; THEN &quot;L&quot; END) as &quot;L_Home&quot;,
count(CASE 
	WHEN wl_away IS &quot;W&quot; AND team_abbreviation_away IS &quot;CLE&quot; THEN &quot;W&quot; END) as &quot;W_Away&quot;,
count(CASE 
	WHEN wl_away IS &quot;L&quot; AND team_abbreviation_away IS &quot;CLE&quot; THEN &quot;L&quot; END) as &quot;L_Away&quot;,
count(CASE
	WHEN wl_home IS &quot;W&quot; AND team_abbreviation_home IS &quot;CLE&quot; OR wl_away IS &quot;W&quot; AND team_abbreviation_away IS &quot;CLE&quot; THEN &quot;W&quot; END) as &quot;W_Total&quot;,
count(CASE
	WHEN wl_home IS &quot;L&quot; AND team_abbreviation_home IS &quot;CLE&quot; OR wl_away IS &quot;L&quot; AND team_abbreviation_away IS &quot;CLE&quot; THEN &quot;L&quot; END) as &quot;L_Total&quot;
FROM
	game
</sql><current_tab id="4"/></tab_sql></sqlb_project>
